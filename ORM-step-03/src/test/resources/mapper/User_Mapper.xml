<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaohe.ORM.dao.IUserDao">

    <select id="queryUserInfoById" parameterType="java.lang.Long" resultType="com.xiaohe.ORM.po.User">
        SELECT id, userId, userHead, createTime
        FROM user
        where id = #{id}
    </select>


</mapper>
